<!DOCTYPE html>
<html>
<head>
    
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <style>
      
        </style>
    
</head>
<body>
    <div class="navbar  navbar-fixed-top navbar-inverse "  style="background-color:dimgrey;color:white;border:none">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
               <p style="font-weight:400; font-size:30px"> @ViewBag.msg</p>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav" style="float:right" >
                    <li><a href="/testing/dashboard" style="color:azure">Dashboard</a></li>
                    <li><a href="/login/logout" style="color:azure">Logout</a></li>
                    
                    
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        </div>
        

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
